<% layout('layouts/boilerplate') %>
    <form action="/parks/<%= park._id%>?_method=PUT" method="POST">
    <div>
        <label for="title">Title</label>
        <input type="text" id="title" name="park[title]" value="<%= park.title %>">
    </div>
    <div>
        <label for="location">Location</label>
        <input type="text" id="location" name="park[location]" value=<%= park.location %>>
    </div>
    <button>Save Edits</button>
    </form>
    <a href="/parks/<%= park._id %>">Back to Park</a>
    <footer>
        <button><a href="/parks">Back to All Parks</a></button>
    </footer>
